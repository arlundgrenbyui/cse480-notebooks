      "source": [
        "# DFA of nfa1\n",
        "\n",
        "dfa1 = md2mc('''DFA\n",
        "\n",
        "I1_I2_B : b | s -> B\n",
        "\n",
        "I1_I2_B : a -> _I2\n",
        "\n",
        "I1_I2_B : f | c -> F3\n",
        "\n",
        "I1_I2_B : d -> F4\n",
        "\n",
        "!!_I2 : a | f | d | s -> H\n",
        "\n",
        "_I2 : c -> F3\n",
        "\n",
        "_I2 : b -> B\n",
        "\n",
        "B : s -> B\n",
        "\n",
        "B : f -> F3\n",
        "\n",
        "B : d -> F4\n",
        "\n",
        "!!B : a | b | c -> H\n",
        "\n",
        "!!F3 : a | b | c | f | d | s -> H\n",
        "\n",
        "!!F4 : a | b | c | f | d | s -> H\n",
        "\n",
        "!!H : a | b | c | f | d | s -> H\n",
        "\n",
        "''')"
      ],
